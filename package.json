{
  "name": "candidate-report",
  "version": "0.2.2",
  "engines": {
    "node": "10"
  },
  "LICENCE": "UNLICENSED",
  "scripts": {
    "ng": "ng",
    "start": "node dist/node/server.js",
    "start:dev": "ng serve",
    "build": "ng build",
    "deploy": "gcloud app deploy --project candidate-report",
    "deploy:nopromote": "gcloud app deploy --no-promote --project candidate-report",
    "deploy:firebase": "firebase deploy",
    "deploy:database": "firebase deploy --only database",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:prod": "npm run build:bundles && npm run build:server",
    "build:bundles": "ng build --prod && ng run candidate-report:server:production",
    "build:client:sm": "ng build --prod --source-map",
    "build:server": "tsc -p server/server.tsconfig.json",
    "smex": "source-map-explorer",
    "stylelint": "stylelint 'src/app/**/*.scss' 'src/*.scss' --config .stylelintrc --syntax scss",
    "update": "npm-check -u -E"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.6",
    "@angular/cdk": "8.2.0",
    "@angular/common": "8.2.6",
    "@angular/compiler": "8.2.6",
    "@angular/core": "8.2.6",
    "@angular/fire": "5.2.1",
    "@angular/forms": "8.2.6",
    "@angular/material": "8.2.0",
    "@angular/platform-browser": "8.2.6",
    "@angular/platform-browser-dynamic": "8.2.6",
    "@angular/platform-server": "8.2.6",
    "@angular/pwa": "^0.803.4",
    "@angular/router": "8.2.6",
    "@angular/service-worker": "8.2.6",
    "@nestjs/common": "6.6.7",
    "@nestjs/core": "6.6.7",
    "@nestjs/ng-universal": "^2.0.1",
    "@nestjs/platform-express": "6.6.7",
    "@nguniversal/common": "8.1.1",
    "@nguniversal/express-engine": "8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "angulartics2": "8.1.0",
    "core-js": "2.6.9",
    "express": "4.17.1",
    "firebase": "6.6.1",
    "hammerjs": "2.0.8",
    "logrocket": "^1.0.3",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.3",
    "ws": "7.1.2",
    "xhr2": "0.2.0",
    "xmlhttprequest": "1.8.0",
    "yes-https": "git+https://github.com/DevIntent/yes-https.git",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.4",
    "@angular/cli": "8.3.4",
    "@angular/compiler-cli": "8.2.6",
    "@angular/language-service": "8.2.6",
    "@types/jasmine": "3.4.0",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "12.7.5",
    "codelyzer": "^5.0.1",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.3.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-firefox-launcher": "1.2.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "npm-check": "5.9.0",
    "protractor": "6.0.0",
