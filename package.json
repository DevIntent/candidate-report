{
  "name": "candidate-report",
  "version": "0.1.0",
  "engines": {
    "node": "8"
  },
  "LICENCE": "UNLICENSED",
  "scripts": {
    "ng": "ng",
    "start": "node dist/node/server.js",
    "start:dev": "ng serve",
    "build": "ng build",
    "deploy": "gcloud app deploy --project candidate-report",
    "deploy:nopromote": "gcloud app deploy --no-promote --project candidate-report",
    "deploy:firebase": "firebase deploy",
    "deploy:database": "firebase deploy --only database",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:prod": "npm run build:bundles && npm run build:server",
    "build:bundles": "ng build --prod && ng run candidate-report:server:production",
    "build:client:sm": "ng build --prod --source-map",
    "build:server": "tsc -p server/server.tsconfig.json",
    "smex": "source-map-explorer",
    "update": "npm-check -u -E"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.0.1",
    "@angular/cdk": "7.0.1",
    "@angular/common": "7.0.1",
    "@angular/compiler": "7.0.1",
    "@angular/core": "7.0.1",
    "@angular/fire": "5.1.0",
    "@angular/flex-layout": "7.0.0-beta.19",
    "@angular/forms": "7.0.1",
    "@angular/http": "7.0.1",
    "@angular/material": "7.0.1",
    "@angular/platform-browser": "7.0.1",
    "@angular/platform-browser-dynamic": "7.0.1",
    "@angular/platform-server": "7.0.1",
    "@angular/router": "7.0.1",
    "@nestjs/common": "5.4.0",
    "@nestjs/core": "5.4.0",
    "@nestjs/ng-universal": "^0.1.1",
    "@nguniversal/common": "7.0.2",
    "@nguniversal/express-engine": "7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "7.0.2",
    "core-js": "2.5.7",
    "express": "4.16.4",
    "hammerjs": "2.0.8",
    "reflect-metadata": "0.1.12",
    "rxjs": "6.3.3",
    "ws": "6.1.0",
    "xhr2": "0.1.4",
    "xmlhttprequest": "1.8.0",
    "yes-https": "git+https://github.com/DevIntent/yes-https.git",
    "zone.js": "0.8.26"
  },
