{
  "name": "candidate-report",
  "version": "0.3.0",
  "engines": {
    "node": "12"
  },
  "LICENCE": "UNLICENSED",
  "scripts": {
    "ng": "ng",
    "start": "node dist/node/server.js",
    "start:dev": "ng serve",
    "build": "ng build",
    "deploy": "gcloud app deploy --project candidate-report",
    "deploy:nopromote": "gcloud app deploy --no-promote --project candidate-report",
    "deploy:firebase": "firebase deploy",
    "deploy:database": "firebase deploy --only database",
    "test": "ng test",
    "lint": "ng lint",
    "lint:styles": "stylelint 'src/app/**/*.scss' 'src/*.scss' --config .stylelintrc --syntax scss",
    "e2e": "ng e2e",
    "build:prod": "npm run build:bundles && npm run build:server",
    "build:bundles": "ng build --prod && ng run candidate-report:server:production",
    "build:client:sm": "ng build --prod --source-map",
    "build:server": "tsc -p server/server.tsconfig.json",
    "smex": "source-map-explorer",
    "update": "npm-check -u"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^9.1.4",
    "@angular/animations": "9.1.4",
    "@angular/cdk": "9.2.2",
    "@angular/common": "9.1.4",
    "@angular/compiler": "9.1.4",
    "@angular/core": "9.1.4",
    "@angular/fire": "6.0.0",
    "@angular/forms": "9.1.4",
    "@angular/material": "9.2.2",
    "@angular/platform-browser": "9.1.4",
    "@angular/platform-browser-dynamic": "9.1.4",
    "@angular/platform-server": "9.1.4",
    "@angular/router": "9.1.4",
    "@angular/service-worker": "9.1.4",
    "@nestjs/common": "7.0.9",
    "@nestjs/core": "7.0.9",
    "@nestjs/ng-universal": "^3.1.0",
    "@nestjs/platform-express": "7.0.9",
    "@nguniversal/common": "9.1.0",
    "@nguniversal/express-engine": "9.1.0",
    "@nguniversal/module-map-ngfactory-loader": "9.0.0-next.9",
    "angulartics2": "9.0.0",
    "express": "4.17.1",
    "express-serve-static-core": "^0.1.1",
    "firebase": "7.14.2",
    "logrocket": "^1.0.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "6.5.5",
    "tslib": "^1.11.1",
    "ws": "^7.2.5",
    "xhr2": "^0.2.0",
    "xmlhttprequest": "^1.8.0",
    "yes-https": "git+https://github.com/DevIntent/yes-https.git",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.4",
    "@angular/cli": "9.1.4",
    "@angular/compiler-cli": "9.1.4",
    "@angular/language-service": "9.1.4",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "13.13.4",
    "codelyzer": "^5.2.2",
    "firebase-tools": "^8.2.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "^5.0.2",
    "karma": "^5.0.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-html-reporter": "1.5.3",
    "npm-check": "5.9.2",
    "protractor": "5.4.4",
    "source-map-explorer": "2.4.2",
    "stylelint": "^13.3.3",
    "ts-node": "8.9.1",
    "tslint": "6.1.2",
    "typescript": "3.8.3"
