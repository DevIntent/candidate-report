    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.3.1",
    "npm-check": "5.9.0",
    "protractor": "5.4.1",
    "source-map-explorer": "1.6.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.1.3"
  }
