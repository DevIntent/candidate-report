{
  "name": "candidate-report",
  "version": "0.0.0",
  "engines": {
    "node": "8"
  },
  "LICENCE": "UNLICENSED",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ssr": "npm --prefix functions start",
    "build": "ng build",
    "deploy": "firebase deploy --only functions",
    "deploy-all": "firebase deploy",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cp:dist": "cp -R functions/dist/browser dist/browser && cp -R functions/dist/server dist/server",
    "serve:node": "npm run compile:server && node dist/node/server.js",
    "build:node": "npm run build:client-and-server-bundles && npm run compile:server && npm run cp:dist",
    "build:ssr": "npm run build:client-and-server-bundles && npm run cp:public && npm run rm:index && npm --prefix functions run build",
    "build:client-and-server-bundles": "ng build --prod && ng run candidate-report:server:production",
    "cp:public": "rm -Rf public/ && mkdir public/ && cp -Rf functions/dist/browser/ public/",
    "rm:index": "rm public/index.html",
    "compile:server": "tsc -p server.tsconfig.json",
    "update": "npm-check -u -E"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/cdk": "^6.4.6",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/material": "^6.4.6",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/platform-server": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@nguniversal/common": "^6.1.0",
    "@nguniversal/express-engine": "^6.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.1.0",
    "core-js": "^2.5.4",
    "express": "^4.15.2",
    "hammerjs": "^2.0.8",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "ws": "^6.0.0",
    "xhr2": "^0.1.4",
    "xmlhttprequest": "^1.8.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.7.5",
    "@angular/cli": "6.1.5",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^4.4.4",
    "jasmine-core": "^3.2.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-firefox-launcher": "~1.1.0",
    "karma-coverage-istanbul-reporter": "~2.0.2",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "~1.3.1",
    "npm-check": "^5.8.0",
    "protractor": "~5.4.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "~2.8.4"
  }
}
